[submodule "nixpkgs"]
	path = nixpkgs
        url = https://github.com/nixos/nixpkgs
        branch = master
[submodule "darwin"]
	path = darwin
	url = https://github.com/LnL7/nix-darwin.git
	branch = master
[submodule "home-manager"]
	path = home-manager
	url = https://github.com/rycee/home-manager.git
	branch = master
[submodule "dotfiles"]
	path = config/dotfiles
